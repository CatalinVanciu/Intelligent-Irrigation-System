<resources>
    <style name="WaterProgressBarTheme">
        <item name="colorAccent">@color/DodgerBlue</item>
    </style>

    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/design_default_color_primary</item>
        <item name="colorPrimaryDark">@color/design_default_color_primary_dark</item>
        <item name="colorAccent">#7E5297</item>
    </style>

    <style name="WaterCircularDeterminateProgressBar">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/circle_water_progress</item>
    </style>

    <style name="UVCircularDeterminateProgressBar">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/circle_uv</item>
    </style>

</resources>